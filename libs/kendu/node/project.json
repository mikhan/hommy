{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/kendu/node/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/kendu/node",
        "tsConfig": "libs/kendu/node/tsconfig.lib.json",
        "packageJson": "libs/kendu/node/package.json",
        "main": "libs/kendu/node/src/index.ts",
        "assets": ["libs/kendu/node/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/kendu/node/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/kendu/node"],
      "options": {
        "jestConfig": "libs/kendu/node/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:lib", "scope:shared", "target:node"]
}
