{
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "tailwindCSS.validate": true,
  "scss.validate": false,
  "editor.quickSuggestions": { "strings": true },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "files.watcherExclude": { "**/dist/**": true, "**/.logs/**": true },
  "search.exclude": {
    "**/dist": true,
    ".yarn/**": true,
    "yarn.lock": true,
    ".logs/**": true
  },
  "files.exclude": {},
  "prettier.prettierPath": "node_modules/prettier/index.js",
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "askForPassword": true,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "Hommy",
      "connectString": "postgresql://postgres:postgres@localhost:5432/hommy?schema=public"
    }
  ],
  "explorer.fileNesting.patterns": {
    "*.component.ts": "${capture}.component.*",
    "*.service.ts": "${capture}.service.*",
    "*.controller.ts": "${capture}.controller.*",
    "*.widget.ts": "${capture}.widget.*",
    "*.page.ts": "${capture}.page.*"
  }
}
